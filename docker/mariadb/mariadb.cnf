[client]
default-character-set = utf8

[mysqld]
character-set-client-handshake = FALSE
character-set-server           = utf8
collation-server               = utf8_unicode_520_ci

max_connections = 800
wait_timeout    = 86400

[mariadb]
general_log = on
general_log_file = /var/log/room-609-mysql.log
